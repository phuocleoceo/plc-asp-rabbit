{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RabbitMQ": {
    "HostName": "localhost",
    "Port": 5672,
    "ExchangeConfigs": [
      {
        "ExchangeName": "plc.exchange",
        "QueueConfigs": [
          {
            "QueueName": "plc.queue.product",
            "RoutingKeys": [
              "plc.key.product.*",
              "plc2.key.product.*"
            ]
          },
          {
            "QueueName": "plc.queue.user",
            "RoutingKeys": [
              "plc.key.user.*",
              "plc2.key.user.*"
            ]
          }
        ]
      }
    ]
  }
}